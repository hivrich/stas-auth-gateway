PORT=3337
NODE_ENV=production

# Public Host (optional, for building URLs/logs)
PUBLIC_BASE_URL=https://intervals.stas.run

# STAS bridge (internal). Requires API key. Gateway injects it itself.
STAS_INTERNAL_BASE_URL=http://127.0.0.1:3336
STAS_API_KEY=REPLACE_WITH_STAS_BRIDGE_KEY

# Intervals ICU base (no global key! Per-user from DB)
ICU_API_BASE_URL=https://intervals.icu/api/v1

# OAuth internals (already configured in your repo)
JWT_PUBLIC_KEY_PEM=-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----
JWT_ALG=RS256

# Database (same DB where oauth tables live)
DATABASE_URL=postgres://user:pass@host:5432/dbname

# ABSOLUTELY NO DEFAULT USER IDS HERE
# DEFAULT_USER_ID=<forbidden>
